@model IEnumerable<RapPhim2101.Models.Phim>
@{
    ViewBag.Title = "test";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    
    @foreach (var i in Model)
    {
        <h2>@i.hinhtraller</h2>
        <p>@i.TenPhim</p>
        <video controls="controls">
            <source src="@i.traller" type="video/mp4" />
            <source src="@i.traller" type="video/webm" />
            <source src="@i.traller" type="video/ogg" />
        </video> 
        <img src="@i.hinhtraller" alt="Alternate Text" />
    }
</div>


